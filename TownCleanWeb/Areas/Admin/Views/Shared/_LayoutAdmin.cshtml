<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <!-- Bootstrap core CSS -->
    <link href="@Url.Content("../../../css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("../../../fonts/css/font-awesome.min.css")" rel="stylesheet">
    <link href="@Url.Content("../../../css/animate.min.css")" rel="stylesheet">
    <link href="@Url.Content("../../../css/custom.css")" rel="stylesheet">
    <!-- Custom styling plus plugins -->
    <link href="@Url.Content("../../../css/icheck/flat/green.css")" rel="stylesheet">
    <script src="@Url.Content("../../../js/jquery.min.js")"></script>
    <!--[if lt IE 9]>
        <script src="../assets/js/ie8-responsive-file-warning.js"></script>
        <![endif]-->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    @RenderSection("JavaScript", required: false)
</head>

<body class="nav-md">
        <div class="container body">
            <div class="main_container">
                <div class="col-md-3 left_col">
                    @Html.Partial("_AdminMenuPartial")
                </div>
                <!-- top navigation -->
                <div class="top_nav">
                    <div class="nav_menu">
                        @Html.Partial("_AdminLoginPartial")
                    </div>
                </div>
                <!-- /top navigation -->
                <div class="right_col" role="main">
                    @RenderBody()
                </div>
            </div>
        </div>
        <div id="custom_notifications" class="custom-notifications dsp_none">
            <ul class="list-unstyled notifications clearfix" data-tabbed_notifications="notif-group"></ul>
            <div class="clearfix"></div>
            <div id="notif-group" class="tabbed_notifications"></div>
        </div>

        <script src="@Url.Content("../../../js/bootstrap.min.js")"></script>
        <!-- bootstrap progress js -->
        <script src="@Url.Content("../../../js/progressbar/bootstrap-progressbar.min.js")"></script>
        <script src="@Url.Content("../../../js/nicescroll/jquery.nicescroll.min.js")"></script>
        <!-- icheck -->
        <script src="@Url.Content("../../../js/icheck/icheck.min.js")"></script>
        <script src="@Url.Content("../../../js/custom.js")"></script>
        <!-- moris js -->
        <script src="@Url.Content("../../../js/moris/raphael-min.js")"></script>
       <!-- pace -->
        <script src="@Url.Content("../../../js/pace/pace.min.js")"></script>    
</body>
</html>
